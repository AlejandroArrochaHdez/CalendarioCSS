      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <children>
      <MonthCalendar id="monthCalendar" fx:id="calendar1" GridPane.rowIndex="1" />
      <MonthCalendar id="monthCalendar" fx:id="calendar2" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <MonthCalendar id="monthCalendar" fx:id="calendar3" GridPane.columnIndex="2" GridPane.rowIndex="1" />
